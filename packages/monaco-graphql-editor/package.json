{
  "name": "@theguild/editor",
  "version": "1.1.1",
  "license": "MIT",
  "type": "module",
  "bob": {
    "commonjs": false
  },
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/typings/index.d.ts",
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      },
      "default": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      }
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "sideEffects": false,
  "peerDependencies": {
    "@monaco-editor/react": "^4",
    "monaco-editor": "^0.24.0 || ^0.25.0 || ^0.26.0 || ^0.27.0 || ^0.28.0 || ^0.29.0 || ^0.30.0 || ^0.32.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "graphql-language-service": "3.2.4"
  },
  "devDependencies": {
    "@monaco-editor/react": "4.3.1",
    "graphql": "16.6.0",
    "monaco-editor": "0.32.1"
  }
}
