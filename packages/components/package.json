{
  "name": "@theguild/components",
  "version": "3.0.3",
  "license": "MIT",
  "type": "module",
  "bob": {
    "commonjs": false,
    "check": false
  },
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/typings/index.d.ts",
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      },
      "default": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      }
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "sideEffects": false,
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@algolia/autocomplete-js": "1.7.1",
    "@algolia/autocomplete-plugin-algolia-insights": "1.7.1",
    "@algolia/autocomplete-plugin-query-suggestions": "1.7.1",
    "@algolia/autocomplete-theme-classic": "1.7.1",
    "@headlessui/react": "1.6.6",
    "@radix-ui/react-navigation-menu": "1.0.0",
    "algoliasearch": "4.14.2",
    "clsx": "1.2.1",
    "focus-trap-react": "9.0.2",
    "nextra-theme-docs": "2.0.0-beta.18",
    "react-instantsearch-dom": "6.31.1",
    "react-paginate": "8.1.3",
    "react-player": "2.10.1",
    "search-insights": "2.2.1",
    "tinykeys": "1.4.0",
    "use-debounce": "8.0.3"
  },
  "devDependencies": {
    "@guild-docs/algolia": "0.2.2",
    "@types/dedent": "0.7.0",
    "@types/react-dom": "18.0.6",
    "dedent": "0.7.0",
    "next": "12.2.5",
    "next-themes": "0.2.0"
  }
}
