{
  "name": "@theguild/components",
  "version": "4.0.1",
  "license": "MIT",
  "type": "module",
  "repository": {
    "url": "the-guild-org/the-guild-components",
    "directory": "packages/components"
  },
  "bob": {
    "check": false,
    "commonjs": false,
    "build": {
      "copy": [
        "./src/static/illustrations/marketplace-desktop.png",
        "./src/static/illustrations/marketplace-mobile.png",
        "./src/static/illustrations/marketplace-cube-tl.png",
        "./src/static/illustrations/marketplace-cube-tr.png",
        "./src/static/illustrations/marketplace-cube-bl.png",
        "./src/static/illustrations/marketplace-cube-br.png",
        "./src/style.css"
      ]
    }
  },
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/typings/index.d.ts",
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  },
  "exports": {
    "./style.css": "./dist/style.css",
    "./search-bar-v2.css": "./dist/search-bar-v2.css",
    ".": {
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      },
      "default": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      }
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "sideEffects": false,
  "peerDependencies": {
    "next": "^12.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@algolia/autocomplete-js": "1.7.1",
    "@algolia/autocomplete-plugin-algolia-insights": "1.7.1",
    "@algolia/autocomplete-plugin-query-suggestions": "1.7.1",
    "@algolia/autocomplete-theme-classic": "1.7.1",
    "@next/bundle-analyzer": "^12.3.1",
    "@radix-ui/react-navigation-menu": "1.0.0",
    "algoliasearch": "4.14.2",
    "clsx": "1.2.1",
    "focus-trap-react": "9.0.2",
    "mermaid": "9.1.7",
    "next-videos": "1.5.0",
    "nextra": "2.0.0-beta.29",
    "nextra-theme-docs": "2.0.0-beta.29",
    "react-instantsearch-dom": "6.32.1",
    "react-paginate": "8.1.3",
    "react-player": "2.10.1",
    "remark-mdx-disable-explicit-jsx": "0.1.0",
    "search-insights": "2.2.1",
    "tiny-lru": "8.0.2",
    "undici": "5.10.0",
    "unist-util-visit": "^4.1.1",
    "use-debounce": "8.0.4"
  },
  "devDependencies": {
    "@theguild/algolia": "0.0.2",
    "@types/dedent": "0.7.0",
    "@types/mermaid": "9.1.0",
    "@types/react-dom": "18.0.6",
    "dedent": "0.7.0",
    "next": "12.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
